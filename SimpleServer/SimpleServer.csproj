<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>SimpleNebulaServer</PackageId>
    <Version>1.0.6</Version>
    <Authors>Nebulaxin</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>
    This is simple single threaded server that can host files and response to HTTP requests

    Source: https://github.com/Nebulaxin/SimpleServer
    </Description>
    <RepositoryUrl>https://github.com/Nebulaxin/SimpleServer</RepositoryUrl>
    <PackageReleaseNotes>Server.Start() method replaced with async Server.Listen()</PackageReleaseNotes>
  </PropertyGroup>
</Project>
